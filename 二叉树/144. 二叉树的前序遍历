class Solution {
    public List<Integer> preorderTraversal(TreeNode root) {
        List<Integer> res = new ArrayList<Integer>();//存储结果集
        preorder(root, res);
        return res;
    }

    public void preorder(TreeNode root, List<Integer> res){
        if(root == null){//终止条件
            return;
        }
        //前序遍历的搜索顺序是中左右
        res.add(root.val);//中
        preorder(root.left, res);//左
        preorder(root.right, res);//右
    }
}
